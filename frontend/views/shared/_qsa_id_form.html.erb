<% define_template "as_runcorn_qsa_id", jsonmodel_definition(model) do |form| %>
  <div class="form-group">
    <label class="col-sm-2 control-label" for="<%= model %>_qsa_id_prefixed_" plugin="">
      <%= I18n.t('as_runcorn.qsa_id') %>
    </label>
    <div class="col-sm-9 label-only">
      <% if form.obj['qsa_id_prefixed'] %>
        <%= QSAIdHelper.id(form.obj['qsa_id_prefixed']) %>
        <%= form.hidden_input 'qsa_id_prefixed', form.obj['qsa_id_prefixed'] %>
      <% else %>
        <span class="text-muted">-- auto-generated upon save --</span>
      <% end %>
    </div>
  </div>
<% end %>

<% form.emit_template("as_runcorn_qsa_id") %>
